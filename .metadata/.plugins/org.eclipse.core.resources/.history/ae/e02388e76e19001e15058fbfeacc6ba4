package namakal;

public class LoopingDemo {

	public static void main(String[] args) {
	LoopingDemo ld=new LoopingDemo();
	ld.pattern();
;	}

	private void pattern() {
		   for(int row = 1; row<=3;row++)
		      {
		      for(int col=1; col<=7; col++)
		      {
		        System.out.print("* ");
		      }
		      System.out.println();
		      }    
		  }
		
		
	}


