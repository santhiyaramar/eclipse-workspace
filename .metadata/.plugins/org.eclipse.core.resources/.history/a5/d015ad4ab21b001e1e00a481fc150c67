package namakal;

public class LoopingDemo1 {

	public static void main(String[] args) {
		LoopingDemo1 lp= new LoopingDemo1();
		lp.pattren_B();
	}

	private void pattren_B() {
		for(int row=1;row<=7;row++)
		{
			for(int col=1; col<=7;col++)
			{
				if(row==1||row==7||col==1)
				{
					System.out.print("* ");
				}
				else
				{
					
					System.out.print("  ");
				}
			}
			System.out.println();
		}
		
	}
}


