package practice;

public class Ifcondition {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=10;
		int sum=0;
		int i = 1;
		while(n<=10)
		{
			//System.out.println("summing:" + sum);
			sum = sum+i;
			//System.out.println("summing:" + sum);
			i++;
		}
		System.out.println("sum of natural number is:" + sum +" "+sum);
	}

}
