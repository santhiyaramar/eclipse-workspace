package execise6;

public class StringDemo {

	public static void main(String[] args) {
	StringDemo str=new StringDemo();
	int[] ar= {10,8,5,1};
	str.Find_biggest(ar);
	

	}

	private void Find_biggest() {
	    for(int i=0; i<ar.length-1; i++)
	    {
	      if(ar[i]>ar[i+1])
	      {
	        int temp = str[i];
	        ar[i] = ar[i+1];
	        ar[i+1] = temp;
	      }
	    }
	    System.out.println("Biggest is "+ str[str.length-1]);
	  }
	}

}
